# Choose one: OPENAI or ANTHROPIC
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here

# Global Model Configuration (consistent with Render)
MODEL_NAME=gpt-4o-mini      # for legacy routes
LLM_DEFAULT_MODEL=gpt-4     # main default for v2 pipeline
MODEL_ANALYZE=o3-mini       # for legacy analyze
MODEL_REFINE=gpt-4o         # for legacy refine

# Step-specific Models (V2 Pipeline) - consistent with Render
LLM_MODEL_EVIDENCE=gpt-4
LLM_MODEL_BRIEF=gpt-4
LLM_MODEL_PROBLEM=gpt-4
LLM_MODEL_SOLUTION=gpt-4
LLM_MODEL_TEAM=gpt-4
LLM_MODEL_MARKET=gpt-4
LLM_MODEL_BUSINESS_MODEL=gpt-4
LLM_MODEL_COMPETITION=gpt-4
LLM_MODEL_STATUS_QUO=gpt-4
LLM_MODEL_GTM=gpt-4
LLM_MODEL_FINANCIAL_PLAN=gpt-4
LLM_MODEL_INVESTOR_SCORE=gpt-4

# Cache and other settings
CACHE_TTL_SECONDS=86400
USE_ASSUMPTIONS_LLM=true

# Development mode - set to true for testing without real API calls
LLM_DRY_RUN=true
