# Choose one: OPENAI or ANTHROPIC
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here
MODEL_NAME=gpt-4o-mini      # or claude-3-5-sonnet
CACHE_TTL_SECONDS=86400

# --- Best Pipeline Models ---
MODEL_ANALYZE=claude-3-5-sonnet-20240620
MODEL_REFINE=gpt-4o
# Optional: bei true wird automatisch der 2. Pass (LLM-Feinschliff) nach Assumptions ausgef√ºhrt
USE_ASSUMPTIONS_LLM=false

# Leitfaden Pipeline v1
MODEL_ANALYZE=o3-mini
MODEL_REFINE=gpt-4o-mini

# ========================================
# V2 Pipeline Configuration
# ========================================

# RateGate Token Management Configuration
RATEGATE_TOKENS_PER_MINUTE=40000
RATEGATE_TOKENS_PER_DAY=1000000
RATEGATE_TOKENS_PER_HOUR=240000
RATEGATE_RESERVE_PERCENTAGE=0.20
RATEGATE_MIN_TOKENS_FOR_REQUEST=1000
RATEGATE_STATS_INTERVAL_MS=30000

# Default LLM Model
LLM_DEFAULT_MODEL=gpt-4o

# Step-specific Model Configuration 
# (Override individual steps to use different models)
LLM_MODEL_EVIDENCE=gpt-4
LLM_MODEL_BRIEF=gpt-4o
LLM_MODEL_PROBLEM=gpt-4o-mini
LLM_MODEL_SOLUTION=gpt-4o-mini
LLM_MODEL_TEAM=gpt-4o-mini
LLM_MODEL_MARKET=gpt-4
LLM_MODEL_BUSINESS_MODEL=gpt-4
LLM_MODEL_COMPETITION=gpt-4o-mini
LLM_MODEL_STATUS_QUO=gpt-4o-mini
LLM_MODEL_GTM=gpt-4o-mini
LLM_MODEL_FINANCIAL_PLAN=gpt-4
LLM_MODEL_INVESTOR_SCORE=gpt-4

# Phase-specific models for large steps (financial_plan, market, gtm)
# If not set, will use the step-specific or default model
LLM_MODEL_FINANCIAL_PLAN_PHASE1=gpt-4o-mini  # For data generation
LLM_MODEL_FINANCIAL_PLAN_PHASE2=gpt-4        # For narrative generation
LLM_MODEL_MARKET_PHASE1=gpt-4o-mini
LLM_MODEL_MARKET_PHASE2=gpt-4
LLM_MODEL_GTM_PHASE1=gpt-4o-mini
LLM_MODEL_GTM_PHASE2=gpt-4o
